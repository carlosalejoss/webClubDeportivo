model Reserva {
    id          String   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
    socioId     String   @db.Uuid
    precio      Float?
    fecha       DateTime
    nombrePista String
    socio       Socio    @relation(fields: [socioId], references: [id])
    pista       Pista    @relation(fields: [nombrePista], references: [nombre])
}

model Pista {
    nombre String @id

    reservas         Reserva[]
}
